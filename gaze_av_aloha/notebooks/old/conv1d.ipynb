   "source": [
    "import torch \n",
    "from torch import nn\n",
    "\n",
    "dim = 64\n",
    "\n",
    "down = nn.Conv1d(dim, dim, 3, 2, 1)\n",
    "\n",
    "up = nn.ConvTranspose1d(dim, dim, 3, 2, 1)\n",
    "\n",
    "x = torch.randn(1, dim, 63)\n",
    "print(x.shape)\n",
    "print(down(x).shape)\n",
    "print(up(down(x)).shape)\n"
   ]
